{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "core/pattern",
	"title": "Pattern",
	"category": "theme",
	"description": "Show a block pattern.",
	"supports": {
		"align": [ "wide", "full" ],
		"__experimentalLayout": true
	},
	"textdomain": "default",
	"attributes": {
		"slug": {
			"type": "string"
		},
		"align": {
			"type": "string",
			"default": "full"
		},
		"layout": {
			"type": "object",
			"default": {
				"type": "constrained"
			}
		},
		"syncStatus": {
			"type": [ "string", "boolean" ],
			"enum": [ "synced", "unsynced" ],
			"default": "synced"
		}
	}
}
